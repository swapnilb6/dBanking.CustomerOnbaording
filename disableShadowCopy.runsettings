<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <RunConfiguration>
    <!-- Disable app domains / shadow-copying so functional tests using WebApplicationFactory can find files in the build output folder -->
    <DisableAppDomain>true</DisableAppDomain>
    <!-- Optional: disable parallel test execution to make file access deterministic -->
    <DisableParallelization>true</DisableParallelization>
  </RunConfiguration>
</RunSettings>